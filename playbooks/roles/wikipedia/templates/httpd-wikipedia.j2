<VirtualHost *:80>
	ServerName www.wikipedia.org
	ServerAlias en.wikipedia.org
	ServerAlias wikipedia.org
	ServerAlias wikipedia
	RewriteEngine on
	RewriteRule ^/wiki(.)(.*)$ /wp/$1/$1$2.htm [PT]
	CheckSpelling on
	Options -Indexes
	DirectoryIndex index.php index.html /notfound.html
	ErrorDocument 404 /notfound.html
	DocumentRoot {{ wikipedia__provision__base_dir }}
</VirtualHost>

